{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "YourDefaultSecretKeyForDevelopmentMinimum32Chars!",
    "Issuer": "HotelBookingAPI",
    "Audience": "HotelBookingAPIUsers",
    "ExpirationInMinutes": 60
  },
  "AWS": {
    "Profile": "default",
    "Region": "us-east-1",
    "SQS": {
      "QueueUrl": "https://sqs.us-east-1.amazonaws.com/YOUR-ACCOUNT-ID/hotel-notifications.fifo"
    }
  }
}
